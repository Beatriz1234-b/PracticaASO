+ # instal apachec
+ sudo apt  intall apache 2
+ #  habilitar un archivo de configuracion 
 + include etc/extra/httpd-vhosts.conf
 + # configurar
 + sudo  nano  /opt/lampp/etc/extra/httpd-vhosts.conf
 + # configurar archivo 
 + sudo nano  /etc/host
 + # crear un archivo index.html
 + sudo nano  /var/ww/html/idex.html
 + # reiniciar
 + sudo systemctl restar apache2
